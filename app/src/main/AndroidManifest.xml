        android:allowBackup="true"
        android:icon="@drawable/student_photo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.Student_login_Activity"></activity>
        <activity android:name=".activity.Manager_Activity"></activity>
        <activity android:name=".activity.Manager_login_Activity"></activity>
        <activity android:name=".activity.Add_student_info"></activity>
        <activity android:name=".activity.Select_student_info"></activity>
        <activity android:name=".activity.Student_total_score"></activity>
        <activity android:name=".activity.Student_activity"></activity>
        <activity android:name=".activity.Manager_register_activity"></activity>
        <activity android:name=".activity.Student_detailed_info"></activity>
    </application>

</manifest>